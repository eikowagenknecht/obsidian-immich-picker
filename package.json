{
	"name": "obsidian-immich-picker",
	"version": "1.1.0",
	"description": "Insert images from a self-hosted Immich photo server",
	"type": "module",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"immich",
		"photos",
		"images"
	],
	"author": "eikowagenknecht",
	"license": "GPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eikowagenknecht/obsidian-immich-picker.git"
	},
	"homepage": "https://github.com/eikowagenknecht/obsidian-immich-picker#readme",
	"bugs": {
		"url": "https://github.com/eikowagenknecht/obsidian-immich-picker/issues"
	},
	"devDependencies": {
		"@eslint/json": "^0.14.0",
		"@popperjs/core": "^2.11.6",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^8.52.0",
		"@typescript-eslint/parser": "^8.52.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"eslint": "^9.39.2",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0"
	}
}
